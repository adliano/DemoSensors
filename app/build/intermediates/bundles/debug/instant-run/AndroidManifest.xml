        android:label="@string/app_name" >
        <activity
            android:name="com.demo.sensors.SensorsActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.demo.sensors.ListSensorsActivity" >
        </activity>
        <activity android:name="com.demo.sensors.DemoAccelerometer" >
