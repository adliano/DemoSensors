        <activity android:name="com.demo.sensors.MagneticSensorActivity" >
        </activity>
        <activity android:name="com.demo.sensors.GyroscopeActivity" >
        </activity>
        <activity android:name="com.demo.sensors.PressureActivity" >
        </activity>
        <activity android:name="com.demo.sensors.ProximitySensorActivity" >
        </activity>
        <activity android:name="com.demo.sensors.TemperatureSensorActivity" >
        </activity>
        <activity android:name="com.demo.sensors.OrientationSensorActivity" >
        </activity>
    </application>

